<odoo>
  <record id="view_work_queue_item_tree" model="ir.ui.view">
    <field name="name">work.queue.item.tree</field>
    <field name="model">work.queue.item</field>
    <field name="arch" type="xml">
      <tree>
        <field name="sequence"/>
        <field name="workorder_id"/>
        <field name="workcenter_id"/>
        <field name="employee_id"/>
        <field name="state"/>
        <field name="production_id"/>
        <field name="product_id"/>
      </tree>
    </field>
  </record>
</odoo>
